{% extends "GeneralLayoutView/GeneralSectionView.html.twig" %}

{% block timeline %}
	<div id="parameterspage">
		<h2>Account<h2>
		<h3>Change your basic account and language settings.</h3>
		{{ data.parametersform.formstart | raw }}
		{{ data.parametersform.pseudo | raw }}
		{{ data.parametersform.email | raw }}
		{{ data.parametersform.lang | raw }}
		{{ data.parametersform.pic | raw }}
		{{ data.parametersform.country | raw }}
		{{ data.parametersform.submit | raw }}
		{{ data.parameterform.formend | raw }}
		<hr>
		<h2>Profile photo<h2>
		<h3>Change your profile photo.</h3>
		<label for="profile-pic">Profile photo</label>
		<input type="file" name="fileToUpload" id="fileToUpload">
		<hr>
		<h2>Password<h2>
		<h3>Change your password settings.</h3>
	</div>
{% endblock %}
